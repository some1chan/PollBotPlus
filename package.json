{
  "name": "pollbotplus",
  "version": "1.1.2",
  "description": "A bot that lets you create  polls on Discord quickly, easily, and beautifully.",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "typeorm": "node ./node_modules/typeorm/cli.js --config ./data/ormconfig.json",
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "start": "node -r dotenv/config dist/Bot.js",
    "start:shard": "node -r dotenv/config dist/index.js",
    "dev": "tsnd -r dotenv/config --transpile-only --respawn ./src/Bot.ts --rs --deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/some1chan/PollBotPlus.git"
  },
  "author": "some1chan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/some1chan/PollBotPlus/issues"
  },
  "homepage": "https://github.com/some1chan/PollBotPlus#readme",
  "devDependencies": {
    "@changesets/cli": "^2.14.1",
    "@types/common-tags": "^1.8.0",
    "@types/node": "^14.14.31",
    "@types/node-emoji": "^1.8.1",
    "@types/node-schedule": "^1.3.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@framedjs/core": "^0.4.0",
    "axios": "^0.21.1",
    "colors": "^1.4.0",
    "common-tags": "^1.8.0",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "hastebin": "^0.2.1",
    "node-emoji": "^1.10.0",
    "node-schedule": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "twitch": "^4.5.0",
    "twitch-auth": "^4.5.0",
    "twitch-chat-client": "^4.5.0",
    "typeorm": "^0.2.31",
    "winston": "^3.3.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.3",
    "erlpack": "github:discord/erlpack",
    "utf-8-validate": "^5.0.4",
    "zlib-sync": "^0.1.7"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}